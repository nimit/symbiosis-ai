      "post": {
        "description": "Send a message (JSON with optional base64 attachments).",
        "parameters": [
          {
            "in": "path",
            "name": "chat_id",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "example": {
                "message": {
                  "text": "Hello there"
                }
              },
              "examples": {
                "multiple-files": {
                  "summary": "Multiple attachments",
                  "value": {
                    "message": {
                      "attachments": [
                        {
                          "data_base64": "\u003cbase64\u003e",
                          "filename": "spec.pdf",
                          "mime_type": "application/pdf"
                        },
                        {
                          "data_base64": "\u003cbase64\u003e",
                          "filename": "image.jpg",
                          "mime_type": "image/jpeg"
                        }
                      ],
                      "text": "Docs attached"
                    }
                  }
                },
                "single-image": {
